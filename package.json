{
  "name": "developmentcypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "test-electron": "cypress run",
    "smoke-test": "cypress run --spec \"cypress\\e2e\\smokeTest\\*.cy.js\"",
    "test-electron-headed": "cypress run --headed",
    "test-chrome-headed": "cypress run --browser chrome --headed",
    "test-dashboard": "npx cypress run --record --key d577d807-b3df-4710-8bc2-fbcc75885313",
    "scripts": "cypress run --spec \"cypress\\e2e\\smokeTest\\*.js\"",
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports && mkdir cypress\\reports\\mocha",    
    "pretest": "npm run clean:reports",
    "combine-reports": "mochawesome-merge ./cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  
  "devDependencies": {
    "cypress": "^13.8.1",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-xpath": "^2.0.1",
    "mocha": "^10.4.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "-": "^0.0.1",
    "D": "^1.0.0",
    "neat-csv": "^5.2.0",
